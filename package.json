{
	"name": "jobless",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"b": "pnpm --filter @jobless/backend",
		"f": "pnpm --filter @jobless/frontend",
		"dev": "pnpm -r --parallel dev",
		"types": "concurrently 'pnpm b types' 'pnpm f types'",
